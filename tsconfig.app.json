{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.ts", "src/**/*.vue", "src/**/*.js"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "module": "ESNext",
    "composite": true,
    "strict": true,
    "allowJs": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": false
  }
}
